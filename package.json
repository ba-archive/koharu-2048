    "mitt": "^3.0.0",
    "pixi-spine": "^4.0.3",
    "pixi.js": "^7.2.3",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/hammerjs": "^2.0.41",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-legacy": "4.0.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
